<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.usmi.csci3130_chat_application">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Csci3130_chat_application">
        <activity
            android:name="com.usmi.csci3130_chat_application.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.usmi.csci3130_chat_application.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.usmi.csci3130_chat_application.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.usmi.csci3130_chat_application.ChatActivity"
            android:exported="false" />
        <activity
            android:name="com.usmi.csci3130_chat_application.UserActivity"
            android:exported="false" />
    </application>

</manifest>